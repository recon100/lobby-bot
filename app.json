{
	"name": "Lobby Slack Bot",
	"description": "A demo bot which allows to use Bandwidth services inside Slack chat",
	"repository": "https://github.com/BandwidthExamples/catapult-slack-bot",
	"logo": "https://avatars2.githubusercontent.com/u/12057884?v=3&s=200",
	"keywords": ["js", "node", "bandwidth", "catapult", "slack"],
	"addons": ["mongolab:sandbox"],
	"env": {
		"SLACK_CLIENT_ID": {
			"description": "Your Slack app client id"
		},
		"SLACK_CLIENT_SECRET": {
			"description": "Your Slack app client secret"
		},
		"SLACK_VERIFICATION_TOKEN": {
			"description": "Your Slack token for webhook verification"
		},
		"NODE_ENV": {
			"value": "production"
		}
	}
}
